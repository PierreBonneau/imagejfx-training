<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytodolist.FXMLDocumentController">
   <children>
      <Label fx:id="title" text="To Do List">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <TextField fx:id="textField" layoutX="32.0" layoutY="16.0" onAction="#addTask" prefHeight="25.0" prefWidth="228.0" />
      <Button fx:id="submitBtn" layoutX="271.0" layoutY="15.0" mnemonicParsing="false" onAction="#addTask" text="Submit new task" />
      <Button fx:id="checkAllBtn" layoutX="30.0" layoutY="182.0" mnemonicParsing="false" onAction="#checkAll" text="Select all" />
      <Button fx:id="deteleBtn" layoutX="280.0" layoutY="181.0" mnemonicParsing="false" onAction="#deleteSelected" text="Delete selected" />
      <ListView fx:id="listView" layoutX="31.0" layoutY="53.0" prefHeight="114.0" prefWidth="368.0" />
   </children>
</AnchorPane>
